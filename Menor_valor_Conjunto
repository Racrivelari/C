#include<stdio.h>

int main(){
    int i, valor = 0, valormaior = 0;
    
    for(i=1;i<=5;i++){
      printf("Digite um valor\n");
      scanf("%d", &valor);
    
        if(valor>valormaior){
            valormaior = valor;
        }
    }

    printf("O maior valor digitado e %d ", valormaior);
}
