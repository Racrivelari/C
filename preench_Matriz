#include <stdio.h>

#define LIN 2
#define COL 2

int main(){

    int a[LIN][COL], i = 0, j = 0;

    for(i=0;i<LIN;i++){
        for(j=0;j<COL;j++){
            scanf("%d", &a[i][j]);
        }
    }

    for(i=0;i<LIN;i++){
        for(j=0;j<COL;j++){
            printf("%4d ", a[i][j]);
        }
        printf("\n");
    }
}            
